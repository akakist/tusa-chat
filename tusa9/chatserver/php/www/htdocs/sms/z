DROP TABLE IF EXISTS `phones`;
CREATE TABLE `phones` (
  `id` int(20) unsigned NOT NULL auto_increment,
  `uid` int(20) unsigned NOT NULL default '0',
  `phone` int(20) not null default 0,
  add_date datetime not null default '2000-01-01',
  PRIMARY KEY  (`id`),
  KEY `uid` (`uid`),
  KEY `phone` (`phone`)
) ;
